<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.2.1.2</Version>
    <Authors>Eldie Turner</Authors>
    <Company>Eldie Turner</Company>
    <Description>This is a basic rest client for interacting with APIs.  It offers the basic functions and is compatable with .net standard 2.0 and .net core 2.0.</Description>
    <PackageProjectUrl>https://github.com/EldieTurner/LittleRestClient</PackageProjectUrl>
    <RepositoryUrl>https://github.com/EldieTurner/LittleRestClient</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>RestClient, Little Rest Client, Easy Rest Client, SImple Rest Client,</PackageTags>
    <NeutralLanguage>en-US</NeutralLanguage>
    <AssemblyVersion>1.2.1.2</AssemblyVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="11.0.2" />
  </ItemGroup>

</Project>
